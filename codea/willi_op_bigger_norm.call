extern long bigger(long a, long b);

RET( bigger( (5<<1), (2<<1) ) == (-1<<1) &&
     bigger( (5<<1), (10<<1) ) == (0<<1) &&
     bigger( (5<<1), (5<<1) ) == (0<<1) &&
     bigger( (-5<<1), (-10<<1) ) == (-1<<1) &&
     bigger( (-10<<1), (-5<<1) ) == (0<<1) &&
     bigger( (-5<<1), (-5<<1) ) == (0<<1) &&
     bigger( (-5<<1), (5<<1) ) == (0<<1) &&
     bigger( (5<<1), (-5<<1) ) == (-1<<1)
     );
